{
  "name": "pie-catalog",
  "version": "1.0.0-prerelease",
  "description": "The Portable Interaction Elements catalog webapp + api",
  "preferGlobal": false,
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/gulp test",
    "prepublish": "npm run build",
    "build": "gulp build",
    "it": "./node_modules/.bin/gulp it",
    "dev": "./node_modules/.bin/nodemon --watch 'src/**/*.ts' --exec './node_modules/.bin/ts-node' src/index.ts",
    "release": "./node_modules/.bin/gulp release",
    "source-maps": "gulp build && ./node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PieLabs/pie-catalog.git"
  },
  "engines": {
    "node": ">=7.2.0",
    "npm": ">=3.10.7"
  },
  "keywords": [
    "pie",
    "catalog"
  ],
  "author": "info@corespring.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PieLabs/pie-catalog/issues"
  },
  "homepage": "https://github.com/PieLabs/pie-catalog#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.8",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.10",
    "release-helper": "PieLabs/release-helper#v1.3.0",
    "run-sequence": "^1.2.2",
    "sinon": "^1.17.7",
    "temp": "^0.8.3",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.4"
  },
  "dependencies": {
    "@types/express": "^4.0.34",
    "@types/fs-extra": "0.0.35",
    "@types/mongodb": "^2.1.36",
    "@types/stack-trace": "0.0.28",
    "@types/winston": "0.0.32",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "gunzip-maybe": "^1.3.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.19",
    "stack-trace": "0.0.9",
    "winston": "^2.3.0"
  }
}
