    "@types/stack-trace": "0.0.28",
    "@types/winston": "0.0.32",
    "aws-sdk": "^2.7.22",
    "bluebird": "^3.4.7",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "gunzip-maybe": "^1.3.1",
    "lodash": "^4.17.4",
    "mime-types": "^2.1.14",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.19",
    "node-fetch": "^1.6.3",
    "pug": "^2.0.0-beta6",
    "resolve": "^1.2.0",
    "semver": "^5.3.0",
    "stack-trace": "0.0.9",
    "tar-stream": "^1.5.2",
    "winston": "^2.3.0"
